    "react-ga": "^2.5.7",
    "react-i18next": "^11.3.4",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "^4.0.0",
    "react-select": "^3.0.8",
    "react-tooltip": "^3.9.2",
    "react-transition-group-plus": "^0.5.3",
